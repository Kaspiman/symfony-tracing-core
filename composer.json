{
  "name": "kaspiman/symfony-tracing-core",
  "type": "symfony-bundle",
  "description": "Symfony OpenTelemetry integration core bundle",
  "keywords": [
    "opentelemetry",
    "tracing",
    "bundle",
    "symfony"
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Yurij Uvarov",
      "email": "yurij.uvarov@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "open-telemetry/sdk": "^1.1",
    "symfony/dependency-injection": "7.1.* | 6.4.*",
    "symfony/service-contracts": "3.5.*",
    "symfony/event-dispatcher": "7.1.* | 6.4.*",
    "symfony/http-kernel": "7.1.* | 6.4.*",
    "symfony/config": "7.1.* | 6.4.*"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Zim\\SymfonyTracingCoreBundle\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3"
  },
  "config": {
    "sort-packages": true,
    "lock": false,
    "allow-plugins": {
      "php-http/discovery": false,
      "tbachert/spi": false
    }
  }
}
